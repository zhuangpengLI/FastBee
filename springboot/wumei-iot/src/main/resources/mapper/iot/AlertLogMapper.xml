        <result property="alertCat"    column="alert_cat"    />
        <result property="familyId"    column="family_id"    />
        <result property="alertTime"    column="alert_time"    />
        
    </resultMap>

    <sql id="selectAlertLogVo">
        select alert_log_id, alert_name, alert_level, status, alert_cat ,family_id,alert_time
        , product_id, product_name, device_id, device_name,user_id, user_name, tenant_id, tenant_name, create_by, create_time, update_by, update_time, remark from iot_alert_log
    </sql>

    <select id="selectAlertLogList" parameterType="com.ruoyi.iot.domain.AlertLog" resultMap="AlertLogResult">
        <include refid="selectAlertLogVo"/>
        <where>
            <if test="userId != null and userId != 0"> and user_id = #{userId}</if>
            <if test="tenantId != null and tenantId != 0"> and tenant_id = #{tenantId}</if>
            <if test="alertName != null  and alertName != ''"> and alert_name like concat('%', #{alertName}, '%')</if>
            <if test="alertLevel != null "> and alert_level = #{alertLevel}</if>
            <if test="status != null "> and status = #{status}</if>
            <if test="alertCat != null "> and alert_cat = #{alertCat}</if>
            <if test="familyId != null "> and family_id = #{familyId}</if>
            <if test="alertTime != null "> and alert_time = #{alertTime}</if>
            <if test="productId != null "> and product_id = #{productId}</if>
            <if test="productName != null  and productName != ''"> and product_name like concat('%', #{productName}, '%')</if>
            <if test="deviceId != null "> and device_id = #{deviceId}</if>
            <if test="deviceName != null  and deviceName != ''"> and device_name like concat('%', #{deviceName}, '%')</if>
            <if test="params.beginCreateTime != null and params.beginCreateTime != '' and params.endCreateTime != null and params.endCreateTime != ''"> and create_time between #{params.beginCreateTime} and #{params.endCreateTime}</if>
        </where>
        order by create_time desc
    </select>
    
    <resultMap type="com.ruoyi.iot.dto.AlertLogAndFamilyWithUser" id="AlertLogWithInfoResult">
