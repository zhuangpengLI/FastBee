    </resultMap>
    <select id="selectAlertLogWithInfoList" parameterType="com.ruoyi.iot.dto.AlertLogAndFamilyWithUser" resultMap="AlertLogWithInfoResult">
       select a.alert_log_id, a.alert_name, a.alert_level, a.status, a.alert_cat ,a.family_id,a.alert_time
        , a.product_id, a.product_name, a.device_id, a.user_id, a.user_name, a.tenant_id, a.tenant_name, a.create_by, a.create_time, a.update_by, a.update_time, a.remark 
        , d.device_name as device_name
        , u.nick_name as nick_name,u.phonenumber as phonenumber
        , f.name as family_name,f.position as family_position
        from iot_alert_log a
        left join iot_device d on d.device_id=a.device_id
        left join iot_family f on f.family_id=a.family_id
        left join sys_user u on u.user_id=a.user_id
        <where>
            <if test="userId != null and userId != 0"> and a.user_id = #{userId}</if>
            <if test="tenantId != null and tenantId != 0"> and a.tenant_id = #{tenantId}</if>
            <if test="alertName != null  and alertName != ''"> and a.alert_name like concat('%', #{alertName}, '%')</if>
            <if test="alertLevel != null "> and a.alert_level = #{alertLevel}</if>
            <if test="status != null "> and a.status = #{status}</if>
            <if test="alertCat != null "> and a.alert_cat = #{alertCat}</if>
            <if test="familyId != null "> and a.family_id = #{familyId}</if>
            <if test="alertTime != null "> and a.alert_time = #{alertTime}</if>
            <if test="productId != null "> and a.product_id = #{productId}</if>
            <if test="productName != null  and productName != ''"> and a.product_name like concat('%', #{productName}, '%')</if>
            <if test="deviceId != null "> and a.device_id = #{deviceId}</if>
            
            <if test="deviceName != null  and deviceName != ''"> and d.device_name like concat('%', #{deviceName}, '%')</if>
            <if test="nickName != null  and nickName != ''"> and u.nick_name like concat('%', #{nickName}, '%')</if>
            <if test="phonenumber != null  and phonenumber != ''"> and u.phonenumber like concat('%', #{phonenumber}, '%')</if>
            
           <if test="params.beginCreateTime != null and params.beginCreateTime != '' and params.endCreateTime != null and params.endCreateTime != ''"> and a.create_time between #{params.beginCreateTime} and #{params.endCreateTime}</if>
            
        </where>
        
        order by a.create_time desc
    </select>
    
    <select id="selectAlertLogByAlertLogId" parameterType="Long" resultMap="AlertLogResult">
        <include refid="selectAlertLogVo"/>
        where alert_log_id = #{alertLogId}
    </select>
        
